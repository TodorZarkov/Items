{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "SignIn": {
        "RequireConfirmedAccount": false
    },

    "Password": {
        "RequireNonAlphanumeric": false,
        "RequireUpperca": false,
        "RequireLowerca": false,
        "RequireDigit": false,
        "RequiredLength": 3
    },

    "User": {
        "RequireUniqueEmail": true
    },

    "Lockout": {
        "DefaultLockoutTimeSpan": 1,
        "MaxFailedAccessAttempts": 5
    }
}
