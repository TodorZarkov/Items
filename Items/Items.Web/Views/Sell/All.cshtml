@model IEnumerable<AllSellViewModel>

@{
	ViewData["Title"] = "My Sellings";
	ViewBag.ActiveLink = "AllSell";
}
@if (Model.Any())
{

	<vc:query-filter hits="85" />


	<table class="table table-warning table-striped table-hover table-sm align-middle">
		<thead>
			<tr>
				<th scope="col">Picture</th>
				<th scope="col">Name</th>
				<th scope="col">Type</th>
				<th scope="col">Category</th>
				<th scope="col">Location</th>
				<th scope="col">Place</th>
				<th scope="col">QTY</th>
				<th scope="col">Highest Bid</th>
				<th scope="col">#Ofers</th>
				<th scope="col">#Bids</th>
				<th scope="col">Price</th>
				<th scope="col">Start</th>
				<th scope="col">End</th>
				<th scope="col" class="ms-auto">Manage</th>
			</tr>
		</thead>

		<partial name="_AllSellsPartial" />

	</table>

}
else
{
	<div style="margin-top: 40vh" class="d-flex align-items-center justify-content-center">
		<h3>You Haven't Put Anything on the Market Yet.</h3>
	</div>
}


